{
    "name": "cundd/custom-rest",
    "type": "typo3-cms-extension",
    "description": "Extension to show how to use the REST extension as Boilerplate",
    "require": {
        "cundd/rest": "*"
    },
    "replace": {
        "custom_rest": "self.version",
        "cundd/custom_rest": "self.version",
        "typo3-ter/custom-rest": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Cundd\\CustomRest\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "custom_rest"
        }
    }
}
